  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/rollup-plugin-vue-i18n.git"
  },
  "scripts": {
    "postinstall": "cd ./node_modules/rollup-plugin-vue && yarn && yarn build && cd ../../",
    "build": "tsc -p .",
    "build:example": "yarn build && rollup -c ./example/basic/rollup.config.js",
    "watch": "tsc -p . --watch",
    "clean": "npm-run-all clean:*",
    "clean:example": "rm -rf ./example/components.esm.js",
    "clean:coverage": "rm -rf ./coverage",
    "coverage": "opener coverage/lcov-report/index.html",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore '**/*.{js,json,html}'",
    "format:fix": "yarn format --write",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "yarn lint --fix",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "test": "npm-run-all lint test:cover",
    "test:cover": "yarn test:unit --coverage",
    "test:unit": "jest --env node",
    "test:watch": "jest --env node --watch"
  },
  "dependencies": {
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "rollup-pluginutils": "^2.8.2"
  }
}
